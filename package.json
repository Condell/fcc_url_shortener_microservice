{
  "name": "fcc_url_shortener_microservice",
  "version": "1.0.0",
  "description": "Free Code Camp Back-end Certification URL Shortener Project",
  "main": "index.js",
  "scripts": {
    "build": "babel server -d production --ignore test.js --plugins transform-remove-console",
    "lint": "esw server --color",
    "lint:watch": "npm run lint -- --watch",
    "serve": "SET NODE_ENV=production & node production/app.js",
    "security-check": "nsp check",
    "prestart": "babel-node buildScripts/startMessage.js server/**/*.js",
    "start": "npm-run-all --parallel security-check test:watch lint:watch",
    "test": "mocha --reporter spec buildScripts/testSetup.js \"server/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "nodemonstart": "nodemon --exec npm start -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Condell/fcc_url_shortener_microservice.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Condell/fcc_url_shortener_microservice/issues"
  },
  "homepage": "https://github.com/Condell/fcc_url_shortener_microservice#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chalk": "^1.1.3",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "mongoose": "^4.9.0",
    "morgan": "^1.8.1"
  }
}
